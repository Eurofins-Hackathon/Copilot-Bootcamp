{
  "name": "-wrightbrothersfrontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "frontend": "vite",
    "backend": "cd ../WrightBrothersApi/WrightBrothersApi && dotnet run",
    "frontend-and-backend": "concurrently \"npm run backend\" \"npm run frontend\"",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test-ct": "playwright test -c playwright-ct.config.ts",
    "test-ct:open": "playwright test -c playwright-ct.config.ts --ui"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.3",
    "@types/react-router-dom": "^5.3.3",
    "autoprefixer": "^10.4.18",
    "axios": "^1.6.8",
    "formik": "2.4.5",
    "gsap": "^3.12.5",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.22.3",
    "styled-components": "^6.1.8",
    "tailwindcss": "^3.4.1",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@playwright/experimental-ct-react": "^1.43.1",
    "@types/node": "^20.12.9",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.2",
    "vite": "^5.1.6"
  }
}
